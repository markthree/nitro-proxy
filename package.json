{
  "version": "0.0.0",
  "name": "nitro-proxy",
  "description": "前端静态服务，支持自动 api 代理",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "nitro-proxy": "./dist/index.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markthree/nitro-proxy.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "pnpm build",
    "build": "unbuild && fast-cpy src/runtime dist/runtime",
    "release": "bumpp --commit --push --tag && npm publish"
  },
  "keywords": [
    "proxy",
    "nitro",
    "node",
    "service"
  ],
  "author": {
    "name": "markthree",
    "email": "1801982702@qq.com",
    "url": "https://github.com/markthree"
  },
  "bugs": {
    "email": "1801982702@qq.com",
    "url": "https://github.com/markthree/nitro-proxy/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.17.11",
    "bumpp": "^9.2.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0"
  },
  "dependencies": {
    "citty": "^0.1.3",
    "fast-cpy": "^0.0.13",
    "nitropack": "^2.6.1",
    "pathe": "^1.1.1",
    "vite-layers": "^0.3.9"
  }
}
